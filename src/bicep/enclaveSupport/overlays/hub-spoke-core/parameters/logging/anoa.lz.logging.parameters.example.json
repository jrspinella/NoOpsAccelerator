{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "parOrgPrefix": {
            "value": "anoa"
        },
        "parTemplateVersion": {
            "value": "v1.0"
        },
        "parOperationsSubscriptionId": {
            "value": "<<subscriptionId>>"
        },
        "parDeployEnvironment": {
            "value": "workload"
        },        
